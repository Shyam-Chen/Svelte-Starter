{
  "name": "Frontend-Starter-Kit",
  "version": "1.0.0",
  "description": "A boilerplate for HTML5, Material, Firebase, Gulp, Rollup, Babel, PostHTML, and PostCSS.",
  "scripts": {
    "start": "gulp --mode dev --watch --serve",
    "test": "gulp polyfills && gulp vendor && gulp unit --watch",
    "dev": "gulp --mode dev --watch --serve",
    "prod": "gulp --mode prod",
    "lint": "gulp lint",
    "unit": "gulp unit",
    "e2e": "gulp e2e",
    "clean": "rimraf public",
    "reset": "git reset --hard HEAD && git clean -df",
    "reinstall": "yarn cache clean && git clean -dfx -e '.*' && yarn install",
    "deploy": "firebase deploy",
    "lib": "gulp polyfills && gulp vendor",
    "webdriver": "gulp webdriver",
    "gulp": "gulp",
    "predev": "rimraf public",
    "preprod": "rimraf public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shyam-Chen/Frontend-Starter-Kit.git"
  },
  "keywords": [
    "HTML5"
  ],
  "author": "Yen-cheng Chen, 陳彥澄",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Shyam-Chen/Frontend-Starter-Kit/issues"
  },
  "homepage": "https://github.com/Shyam-Chen/Frontend-Starter-Kit#readme",
